/*
 * generated by ansible
 */

object Endpoint NodeName {
  host = NodeName
}

object Zone ZoneName {
  endpoints = [ NodeName ]
  parent = "{{ icinga2_parent_zone }}"
}

{% if icinga2_node_global_zone %}
object Zone "{{ icinga2_node_global_zone }}" {
  global = true
}
{% endif %}

object Endpoint "{{ icinga2_parent_endpoint }}" {
  host = "{{ icinga2_parent_host }}"
  port = "{{ icinga2_parent_port }}"
}

object Zone "{{ icinga2_parent_zone }}" {
  endpoints = [ "{{ icinga2_parent_endpoint }}" ]
}
