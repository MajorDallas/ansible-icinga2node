/*
 * generated by ansible
 */

object Endpoint "{{ icinga2_master_endpoint }}" {
        host = "{{ icinga2_master_host }}"
        port = "{{ icinga2_master_port }}"
}

object Zone "{{ icinga2_master_zone }}" {
        endpoints = [ "{{ icinga2_master_endpoint }}" ]
}

object Endpoint NodeName {
}

object Zone "{{ ansible_nodename }}" {
        endpoints = [ NodeName ]
        parent = "{{ icinga2_master_zone }}"
}
{% if icinga2_global_zone %}
object Zone "{{ icinga2_global_zone}}" {
        global = true
}
{% endif %}
